<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="5dp"/>
            <gradient android:startColor="#da050505" android:endColor="#da050505" android:angle="270" android:centerY="1" android:centerColor="#da050505"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="5dp"/>
                <gradient android:startColor="#97000000" android:endColor="#97000000" android:angle="270" android:centerY="1" android:centerColor="#97000000"/>
            </shape>
        </clip>
    </item>
</layer-list>
